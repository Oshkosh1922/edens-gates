# Supabase Configuration (Required)
# Get these values from your Supabase project dashboard
# https://app.supabase.com/project/YOUR_PROJECT_ID/settings/api

VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Feature Flags (Optional - Default: false)
# Wallet Integration: Enable Solana wallet connection for votes
VITE_WALLET_ENABLED=true

# Edge Functions: Use secure server-side functions for admin operations
VITE_USE_EDGE_FUNCTIONS=false

# Media Uploads: Enable image and PDF uploads via Supabase Storage
VITE_UPLOADS_ENABLED=false

# On-Chain Voting Configuration (Required when VITE_WALLET_ENABLED=true)
# Magic Eden $ME token mint address
VITE_ME_MINT=MEFNBXixkEbait3xn9bkm8WsJzXtVsaJEn4c8Sam21u

# Edens Gates rewards wallet pubkey
VITE_REWARDS_VAULT=8rjP7XVxt1eVioDJoLSMMBnse1QAo9SCkoYmVN3t8KAE

# Solana cluster (devnet for testing, mainnet-beta for production)
VITE_CLUSTER=devnet